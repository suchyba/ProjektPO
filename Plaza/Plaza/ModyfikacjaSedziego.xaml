<Window x:Name="DodajZawodnika" x:Class="Plaza.ModyfikacjaSedziego"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Plaza"
        mc:Ignorable="d"
        Title="Modyfikacja Sędziego" Height="300" Width="300">
    <Grid x:Name="OknoSedziego">
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="162" Margin="10,10,0,0" VerticalAlignment="Top" Width="272">
            <TabItem x:Name="DodajSedziego" Header="Dodaj" Margin="0,0,-4,-2" HorizontalAlignment="Left" VerticalAlignment="Top">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem x:Name="UsunSedziego" Header="Usuń" Margin="0" HorizontalAlignment="Left" VerticalAlignment="Top">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem x:Name="ModyfikujSedziego" Header="Modyfikuj" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,-35,0" Width="100">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
