<Window x:Name="OknoZawodnika" x:Class="Plaza.ModyfikacjaZawodnika"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Plaza"
        mc:Ignorable="d"
        Title="Modyfikacja Zawodnika" Height="300" Width="300">
    <Grid>
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="100" Margin="10,10,0,0" VerticalAlignment="Top" Width="272">
            <TabItem x:Name="DodajZawodnika" Header="Dodaj" Margin="0" HorizontalAlignment="Left" VerticalAlignment="Top">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem x:Name="UsunZawodnika" Header="Usuń" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="54" Margin="0"/>
            <TabItem x:Name="ModyfikujZawodnika" Header="Modyfikuj" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="100" Margin="0">
                <Grid Background="#FFE5E5E5">
                </Grid>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
